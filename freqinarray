package sk;

public class sumofndigits {

	public static void main(String[] args) {
		int a[]= {1,2,3,4,1,2,4};
        int[] freq = new int[a.length];
        for (int i=0;i<a.length;i++) 
        	freq[a[i]]++;
        for (int i = 0; i < a.length ; i++)
            if (freq[i] > 0) {
            	System.out.println(i + ": " + freq[i]);
            }
	}

}
