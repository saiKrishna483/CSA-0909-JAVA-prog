package sk;

public class num1223334444333 {

	public static void main(String[] args) {
		int i,j;
		for(i=1;i<=3;i++)
		{
			for(j=1;j<=i;j++)
			{
				System.out.print(i);	
			}
			System.out.println();
		}
		for(i=2;i>=1;i--) {
			for(j=1;j<=i;j++) {
				System.out.print(i);
			}
			System.out.println();
		}
	}

}
