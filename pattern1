package sk;

public class pattern1 {

	public static void main(String[] args) {
		int i,j;
		for(i=1;i<=3;i++)
		{
			for(j=1;j<=i;j++)
			{
				System.out.print("1");	
			}
			System.out.println();
		}
		for(i=2;i>=1;i--)
		{
			for(j=1;j<=i;j++)
			{
				System.out.print("1");	
			}
			System.out.println();
		}

	}

}
